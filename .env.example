# Environment Configuration
ENVIRONMENT=<development or production>

# EmailJS Configuration
# Get these values from your EmailJS dashboard: https://www.emailjs.com/
PUBLIC_EMAILJS_PUBLIC_KEY=your_emailjs_public_key_here
PUBLIC_EMAILJS_SERVICE_ID=your_emailjs_service_id_here
PUBLIC_EMAILJS_TEMPLATE_ID=your_emailjs_template_id_here

# Netlify Secret Scan Omit Keys (To bypass secret scanning for these keys)
SECRETS_SCAN_OMIT_KEYS=PUBLIC_EMAILJS_PUBLIC_KEY,PUBLIC_EMAILJS_SERVICE_ID,PUBLIC_EMAILJS_TEMPLATE_ID,ENVIRONMENT
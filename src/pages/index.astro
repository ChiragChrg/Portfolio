---
import { Layout } from "@layouts/index";
import { Hero, About, Skills, Projects, Contact } from "@components/index";

const {
	themeConfig,
	siteConfig,
	headerConfig,
	heroConfig,
	aboutConfig,
	footerConfig,
} = await import("@/config");
---

<Layout
	config={{
		theme: themeConfig,
		site: siteConfig,
		header: headerConfig,
		footer: footerConfig,
	}}
>
	<Hero config={heroConfig} />
	<About config={aboutConfig} />
	<Skills />
	<Projects />
	<Contact />
</Layout>

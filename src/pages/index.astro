---
import { Layout } from "@layouts/index";
import { Hero, About, Skills, Projects, Contact } from "@components/index";

const { headerConfig, heroConfig, aboutConfig, footerConfig } = await import(
	"@/portfolio.config.ts"
);
---

<Layout
	config={{
		header: headerConfig,
		footer: footerConfig,
	}}
>
	<Hero config={heroConfig} />
	<About config={aboutConfig} />
	<Skills />
	<Projects />
	<Contact />
</Layout>

---
import { Layout } from "@layouts/index";
import { Hero, About, Skills, Projects, Contact } from "@components/index";

const { siteConfig, headerConfig, heroConfig, aboutConfig, footerConfig } =
	await import("@/config");
---

<Layout
	config={{
		site: siteConfig,
		header: headerConfig,
		footer: footerConfig,
	}}
>
	<Hero config={heroConfig} />
	<About config={aboutConfig} />
	<Skills />
	<Projects />
	<Contact />
</Layout>

{
    "branches": [
        "main",
        {
            "name": "astro",
            "prerelease": true
        }
    ],
    "plugins": [
        [
            "@semantic-release/commit-analyzer",
            {
                "preset": "conventionalcommits",
                "releaseRules": [
                    {
                        "type": "enhance",
                        "release": "minor"
                    },
                    {
                        "type": "seo",
                        "release": "minor"
                    },
                    {
                        "type": "perf",
                        "release": "minor"
                    },
                    {
                        "type": "enhance",
                        "release": "minor"
                    },
                    {
                        "type": "docs",
                        "release": "patch"
                    },
                    {
                        "type": "refactor",
                        "release": "patch"
                    },
                    {
                        "type": "style",
                        "release": "patch"
                    },
                    {
                        "type": "chore",
                        "release": "patch"
                    }
                ]
            }
        ],
        [
            "@semantic-release/release-notes-generator",
            {
                "preset": "conventionalcommits",
                "presetConfig": {
                    "types": [
                        {
                            "type": "feat",
                            "section": "‚ú® Features"
                        },
                        {
                            "type": "enhance",
                            "section": "‚≠ê Enhancements"
                        },
                        {
                            "type": "fix",
                            "section": "üêõ Bug Fixes"
                        },
                        {
                            "type": "refactor",
                            "section": "‚ôªÔ∏è Code Refactoring"
                        },
                        {
                            "type": "docs",
                            "section": "üìù Documentation"
                        },
                        {
                            "type": "style",
                            "section": "üé® Styles"
                        },
                        {
                            "type": "seo",
                            "section": "üîç SEO Updates"
                        },
                        {
                            "type": "perf",
                            "section": "üöÄ Performance Improvements"
                        }
                    ]
                }
            }
        ],
        [
            "@semantic-release/npm",
            {
                "npmPublish": false
            }
        ],
        [
            "@semantic-release/changelog",
            {
                "changelogFile": "CHANGELOG.md",
                "changelogTitle": "# üì¶ Changelog\n\nAll notable changes to this project will be documented in this file."
            }
        ],
        [
            "@semantic-release/git",
            {
                "assets": [
                    "package.json",
                    "CHANGELOG.md"
                ],
                "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
            }
        ],
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "dist.zip",
                        "label": "Distribution Build (dist.zip)"
                    }
                ]
            }
        ]
    ]
}